{
  "name": "combine-dependant-reducers",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/combine-dependant-reducers.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/combine-dependant-reducers.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist es",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build",
    "test": "cross-env BABEL_ENV=commonjs NODE_ENV=test mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josepot/combine-dependant-reducers.git"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "keywords": [
    "redux",
    "reducers",
    "higher order reducers",
    "combine reducers",
    "combine higher order reducers"
  ],
  "author": "Josep M Sobrepere (https://github.com/josepot)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josepot/combine-dependant-reducers/issues"
  },
  "homepage": "https://github.com/josepot/combine-dependant-reducers",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "cross-env": "^3.2.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "invariant": "^2.2.2"
  }
}
